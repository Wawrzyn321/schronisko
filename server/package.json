{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "check": "tsc && prettier --check \"src/**/*.ts\"",
    "lint:fix": "prettier --write \"src/**/*.ts\"",
    "start:nest": "REDIS_URL=redis://127.0.0.1:6379 NODE_ENV=production nest start",
    "dev": "PEPPER=PEPPER REDIS_URL=redis://127.0.0.1:6379 NODE_ENV=development nest start --watch --preserveWatchOutput",
    "dev:e2e": "concurrently 'npm run dev' 'npm run redis'",
    "redis": "redis-server",
    "dev:test": "dotenv -e ../prisma/.env.test -- nest start --watch",
    "test": "REDIS_URL='DISABLED' DATABASE_URL='postgres://DONT USE IT' jest",
    "format": "prettier --write \"**/*.ts\""
  },
  "dependencies": {
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@sentry/node": "^8.4.0",
    "@sentry/profiling-node": "^8.4.0",
    "nodemailer": "^6.7.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "postmark": "^4.0.5",
    "random-seed": "^0.3.0",
    "redis": "^4.6.14",
    "sanitize-html": "^2.13.0",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^6.4.15",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/random-seed": "^0.3.5",
    "@types/sanitize-html": "^2.11.0",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "prettierPath": null,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
